    Max = 30,
    Default = 10,
    Rounding = 1,
    Callback = function(Value)
        hitboxsize = Value
        for HRP, sphere in pairs(Visuals) do
            sphere.Size = Vector3.new(hitboxsize, hitboxsize, hitboxsize)
            HRP.Size = Vector3.new(hitboxsize, hitboxsize, hitboxsize)
        end
    end
})

